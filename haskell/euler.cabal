benchmark bench-problems
    type:       exitcode-stdio-1.0
    hs-source-dirs: testsuite/benchmarks, src
    main-is:        problems.hs
    build-depends:  base, 
                    euler, 
                    array,
                    containers, 
                    criterion, 
                    digits, 
                    primes, 
                    split 
    default-language: Haskell2010

benchmark bench-euler
    type:       exitcode-stdio-1.0
    hs-source-dirs: testsuite/benchmarks, src
    main-is:        prime.hs
    build-depends:  base, 
                    euler, 
                    array,
                    containers, 
                    criterion, 
                    digits, 
                    primes, 
                    split 
    default-language: Haskell2010

